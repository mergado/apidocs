                "project_element_id": "1",
                "regular_expression": "(\\-|\\* )?(.*)",
                "sample_text": null,
                "fragment_number": 2
            }

+ Response 201

    + Headers

            Content-Type: application/json
            Location: "https://app.mergado.com/api/variables/1521/"

    + Body

            {
                "id": "1521",
